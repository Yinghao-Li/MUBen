    "version": "0.2.0",
    "configurations": [
        {
            "name": "data-constr",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/data/src/data_processing.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "args": [
                "${workspaceFolder}/scripts/dataset_construction_config.json"
            ]
        },
        {
            "name": "run-grover",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run_grover.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "1",
            },
            "args": [
                "${workspaceFolder}/scripts/grover_config.json"
            ]
        },
        {
            "name": "run-unimol",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run_unimol.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "1",
            },
            "args": [
                "${workspaceFolder}/scripts/unimol_config.json"
            ]
        },
    ]
}