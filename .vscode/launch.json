    "version": "0.2.0",
    "configurations": [
        {
            "name": "data-constr",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/data_constr/build_datasets_from_unimol.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceRoot}",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "args": [
                "--unimol_data_path", "./data/UniMol",
                "--output_dir", "./data/files"
            ]
        },
        {
            "name": "result-org",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/assist/organize_results.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceRoot}",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "args": ["${workspaceFolder}/scripts/config_organize.json"]
        },
        {
            "name": "result-metrics",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/assist/result_metrics.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceRoot}",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "args": ["${workspaceFolder}/scripts/config_metrics.json"]
        },
        {
            "name": "move-models",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/assist/move_models.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceRoot}",
                "CUDA_VISIBLE_DEVICES": "3",
            },
            "args": [
                "--model_name", "GROVER",
                "--feature_type", "rdkit",
                "--result_folder", "./output/",
                "--overwrite_folder",
            ]
        },
        {
            "name": "run-dnn",
            "type": "python",
